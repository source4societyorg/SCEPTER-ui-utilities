{
  "name": "@source4society/scepter-ui-utilities",
  "version": "1.0.0",
  "description": "Utilities for the universal react user interface, boilerplate extracted from react-boilerplate and packaged in a modular library",
  "main": "lib/index.js",
  "repository": "git@github.com:source4societyorg/SCEPTER-ui-utilities.git",
  "author": "Source 4 Society <accounts@source4society.org>",
  "license": "AGPL-3.0",
  "private": false,
  "scripts": {
    "test": "eslint ./src/*.js --ignore-path .gitignore && jest --coverage",
    "build": "babel src/*.js --minified --out-file lib/index.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@source4society/scepter-app-utilities": "^2.0.3",
    "babel-polyfill": "6.23.0",
    "exports-loader": "^0.6.4",
    "history": "^4.7.2",
    "hoist-non-react-statics": "2.1.1",
    "immutable": "^3.8.2",
    "lodash": "4.17.5",
    "react": "15.6.1",
    "react-test-renderer": "15.6.1",
    "redux-saga": "^0.16.0",
    "webpack": "3.5.5",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "^22.4.1",
    "babel-loader": "7.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-styled-components": "1.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.5",
    "babel-preset-env": "1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jest-cli": "21.0.0",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html",
      "cobertura"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 99,
        "branches": 99,
        "functions": 99,
        "lines": 99
      }
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test-bundler.js",
    "testRegex": "tests/.*\\.test\\.js$"
  }
}
